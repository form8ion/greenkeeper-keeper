{
  "name": "greenkeeper-keeper",
  "description": "instance of greenkeeper-keeper for managing greenkeeper PRs automatically",
  "license": "MIT",
  "private": true,
  "repository": "form8ion/greenkeeper-keeper",
  "bugs": "https://github.com/form8ion/greenkeeper-keeper/issues",
  "homepage": "https://github.com/form8ion/greenkeeper-keeper#readme",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "engines": {
    "node": "12.x.x"
  },
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:js": "eslint . --cache",
    "lint:gherkin": "gherkin-lint",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml",
    "lint:engines": "check-engine",
    "clean": "rimraf ./lib",
    "start": "node ./lib/server",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:server": "webpack --env production --config webpack.config.server.babel.js",
    "pretest:integration": "run-s build",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'"
  },
  "dependencies": {
    "@hapi/glue": "6.2.0",
    "@hapi/good": "8.2.4",
    "@travi/hapi-github-webhooks": "3.0.1",
    "bunyan": "1.8.12",
    "dotenv-safe": "8.2.0",
    "good-bunyan": "3.0.1",
    "hapi-graceful-shutdown-plugin": "2.0.8",
    "hapi-greenkeeper-keeper": "3.0.4"
  },
  "devDependencies": {
    "@babel/register": "7.9.0",
    "@form8ion/babel-preset": "1.6.17",
    "@form8ion/commitlint-config": "1.0.9",
    "@form8ion/eslint-config": "1.6.15",
    "@form8ion/eslint-config-cucumber": "1.4.0",
    "@form8ion/remark-lint-preset": "1.0.2",
    "@travi/any": "2.0.7",
    "ban-sensitive-files": "1.9.7",
    "chai": "4.2.0",
    "check-engine": "1.8.1",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.1.0",
    "gherkin-lint": "3.4.0",
    "http-status-codes": "1.4.0",
    "husky": "4.2.3",
    "npm-run-all": "4.1.5",
    "remark-cli": "7.0.1",
    "rimraf": "3.0.2",
    "travis-lint": "1.0.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.10"
  }
}
